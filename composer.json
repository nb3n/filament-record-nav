{
    "name": "nben/filament-record-nav",
    "type": "library",
    "description": "Next/Previous record navigation for Filament PHP",
    "keywords": ["filament", "laravel", "navigation", "records"],
    "homepage": "https://github.com/nb3n/filament-record-nav",
    "support": {
        "issues": "https://github.com/nb3n/filament-record-nav/issues",
        "source": "https://github.com/nb3n/filament-record-nav",
        "docs": "https://github.com/nb3n/filament-record-nav#readme"
    },
    "suggest": {
        "filament/spatie-laravel-media-library-plugin": "For enhanced media management in your resources",
        "filament/spatie-laravel-settings-plugin": "For application settings management", 
        "spatie/laravel-activitylog": "For tracking record changes and navigation history",
        "spatie/laravel-permission": "For role-based access control in your admin panel"
    },
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "filament/filament": "^3.2"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nben\\FilamentRecordNav\\": "src/"
        }
    },
    
    "extra": {
        "laravel": {
            "providers": [
                "Nben\\FilamentRecordNav\\FilamentRecordNavServiceProvider"
            ],
            "aliases": {
                "FilamentRecordNav": "Nben\\FilamentRecordNav\\Facades\\FilamentRecordNav"
            }
        }
    },
    "authors": [
        {
            "name": "nb3n",
            "email": "hello@nben.com.np",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
